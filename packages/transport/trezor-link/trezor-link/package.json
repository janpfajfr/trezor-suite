{
  "name": "trezor-link",
  "version": "0.2.30",
  "description": "Trezor Link",
  "main": "lib/index.js",
  "scripts": {
    "preversion": "make npm_preversion",
    "version": "make npm_version",
    "postversion": "make npm_postversion",
    "build": "make build",
    "check": "make check"
  },
  "repository": "https://github.com/trezor/trezor-link.git",
  "author": "Karel BÃ­lek <kb@karelbilek.com>",
  "license": "LGPL-3.0+",
  "babel": {
    "env": {
      "srctolib": {
        "presets": [
          "modern"
        ],
        "plugins": [
          "transform-flow-strip-types",
          "transform-decorators-legacy",
          "transform-class-properties",
          "transform-object-rest-spread",
          "add-module-exports",
